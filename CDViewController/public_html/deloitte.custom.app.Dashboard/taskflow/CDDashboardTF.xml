<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="CDDashboardTF">
    <default-activity>DashboardPG</default-activity>
    <managed-bean id="__1">
    <managed-bean-name>SlidingDrawerBean</managed-bean-name>
    <managed-bean-class>deloitte.custom.app.application.SlidingDrawerBean</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
    <managed-bean id="__2">
    <managed-bean-name>CloudManagedBean</managed-bean-name>
    <managed-bean-class>deloitte.custom.app.application.CloudManagedBean</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
    <view id="DashboardPG">
      <page>/deloitte.custom.app.Dashboard/Page/DashboardPG.amx</page>
    </view>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
